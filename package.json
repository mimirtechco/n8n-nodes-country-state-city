{
  "name": "n8n-nodes-country-state-city",
  "version": "0.1.0",
  "description": "n8n node for country-state-city",
  "license": "MIT",
  "author": {
    "name": "Andre Kutianski"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mimirtechco/n8n-nodes-country-state-city.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "country",
    "state",
    "city"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "sync-version": "git describe --tags --abbrev=0 | sed 's/^v//' | xargs npm version --no-git-tag-version",
    "release": "npm run sync-version && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/CountryStateCity/CountryStateCity.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.18.45",
    "@typescript-eslint/parser": "~5.45",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "country-state-city": "^3.1.2"
  }
}
