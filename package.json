{
  "name": "n8n-nodes-country-state-city",
  "version": "0.1.0",
  "description": "n8n node for country-state-city",
  "license": "MIT",
  "homepage": "https://docs.n8n.io/integrations/community-nodes/",
  "author": {
    "name": "Andre Kutianski"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mimirtechco/n8n-nodes-country-state-city.git"
  },
  "keywords": [
    "n8n-community-node-package",
    "country",
    "state",
    "city"
  ],
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=8.6"
  },
  "packageManager": "pnpm@8.6.2",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/CountryStateCity/CountryStateCity.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "tslint": "^6.1.2",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "country-state-city": "^3.1.2"
  }
}
